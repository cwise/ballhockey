<%= error_messages_for 'player' %>
<% form_for :player do|f| %>
  <table>
    <caption>Player</caption>
    <tr>
      <th>Name</th>
      <th>Email Address</th>
      <th>Active</th>
    </tr>
    <tr>
      <td><%= f.text_field :name %></td>
      <td><%= f.text_field :email_address %></td>
      <td><%= f.check_box :active %></td>
    </tr>
  </table>

  <%= entry_nav %>
<% end %>
